{
	"$schema": "http://json-schema.org/schema#",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"git": {
				"type": "string",
				"title": "git commit hash",
				"pattern": "^[0-9a-f]{7,40}$"
			},
			"desc": {
				"type": "string",
				"title": "free style text to mention package name"
			},
			"obsolete": {
				"type": "string",
				"title": "Obsoleting UCS release evrsion",
				"pattern": "^[1-9][0-9]*[.-][0-9]+[.-][0-9]+$"
			},
			"vars": {
				"type": "array",
				"title": "UCR variable patterns",
				"items": {
					"type": "string"
				}
			}
		},
		"required": [
			"git",
			"vars"
		],
		"additionalProperties": true
	}
}
