@require "../variables.styl"


.umcPage:after
	content: ''
	display: block
	clear: both

.umcPageFooter
	display: flex
	justify-content: space-between
	flex-wrap: wrap
	grid-gap: spacing(4) spacing(2)

.umcPageFooterLeft,
.umcPageFooterRight
	display: flex
	flex-wrap: wrap
	grid-gap: spacing(2)

.umcPageFooterRight
	margin-left: auto
